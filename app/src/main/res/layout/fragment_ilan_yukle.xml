<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ilanYukleFragment">

    <EditText
        android:id="@+id/ilanmarkaText"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Aracın Markası"
        android:inputType="text"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="439dp" />

    <EditText
        android:id="@+id/ilanmodelText"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Aracın Modeli"
        android:inputType="text"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="484dp" />

    <EditText
        android:id="@+id/ilanfiyatText"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Aracın fiyatı"
        android:inputType="text"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="529dp" />

    <EditText
        android:id="@+id/ilanyakitText"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Aracın yakıtı"
        android:inputType="text"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="574dp" />

    <EditText
        android:id="@+id/ilanmotorGucText"
        android:layout_width="297dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Aracın motor gücü"
        android:inputType="text"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="619dp" />

    <EditText
        android:id="@+id/ilanhpText"
        android:layout_width="305dp"
        android:layout_height="53dp"
        android:ems="10"
        android:hint="Aracın beygiri"
        android:inputType="text"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="664dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/car"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="57dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="277dp"
        android:layout_height="143dp"
        android:src="@android:drawable/ic_input_add"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="214dp" />

    <Button
        android:id="@+id/ilanYuklebutton"
        android:layout_width="200dp"
        android:layout_height="38dp"
        android:text="İlanı Yükle"
        app:layout_constraintBottom_toTopOf="@+id/ilanmarkaText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11" />
</androidx.constraintlayout.widget.ConstraintLayout>